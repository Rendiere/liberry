{
  "name": "based_new",
  "version": "1.0.0",
  "main": "dist/main/main.js",
  "scripts": {
    "start:renderer": "vite",
    "watch:main": "tsc -w -p ./tsconfig.json",
    "electron:serve": "wait-on tcp:5173 && NODE_ENV=development electron .",
    "dev": "concurrently \"npm run watch:main\" \"npm run start:renderer\" \"npm run electron:serve\"",
    "build:renderer": "vite build",
    "build:main": "tsc -p ./tsconfig.json",
    "build": "npm run build:renderer && npm run build:main",
    "prisma:seed": "ts-node prisma/seed/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@vicons/ionicons5": "^0.12.0",
    "naive-ui": "^2.40.1",
    "vue": "^3.5.10"
  },
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^9.0.1",
    "electron": "^31.1.2",
    "nodemon": "^3.1.7",
    "prisma": "^5.20.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6",
    "wait-on": "^8.0.1"
  }
}